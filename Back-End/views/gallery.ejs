        var child3 = document.createElement("svg");
        child3.innerHTML='<svg width="22" height="22" viewBox="0 0 22 22" fill="none" xmlns="http://www.w3.org/2000/svg"> <circle cx="11.1611" cy="11.0654" r="10.4689" fill="#313741"/> <line x1="7.88266" y1="7.41218" x2="14.9779" y2="14.5074" stroke="white" stroke-width="4" stroke-linecap="round"/> <line x1="2" y1="-2" x2="12.0342" y2="-2" transform="matrix(-0.707107 0.707107 0.707107 0.707107 17.2679 7.41218)" stroke="white" stroke-width="4" stroke-linecap="round"/> </svg>';

        parent.appendChild(child2);
        parent.appendChild(child3);
        document.querySelector("#Filters").appendChild(parent);
        console.log("added");
    }
    function RemoveFilter(name){
        var child = document.getElementById(name);
